{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "space-before-function-paren": [
            2,
            "always"
        ],
        "func-names": [
            0
        ]
    },
    "globals": {
        "browser",
        "element",
        "by",
        "$",
        "$$"
    },
    "env": {
        "es6": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "eslint:recommended",
        "defaults/configurations/airbnb"
    ]
}
